  "dependencies": {
    "binaryen": "^120.0.0",
    "esbuild": "^0.24.0",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "purescript": "^0.15.15",
    "purs-tidy": "^0.11.0",
    "spago": "^0.93.41",
    "tailwindcss": "^3.4.14"
  }
}
